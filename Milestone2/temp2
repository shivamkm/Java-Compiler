
TypeName : IDENTIFIER
{
  $$ = (char*)malloc(64);
  snprintf($$, 64, "TypeName_%ld", counter);
  counter++;
  printf("label_%ld [label = \"%s\"]\n", label_counter, $1);
  printf(" %s -> label_%ld\n", $$, label_counter);
  label_counter++;
}
| temporary0 IDENTIFIER
{
  $$ = (char*)malloc(64);
  snprintf($$, 64, "TypeName_%ld", counter);
  counter++;
  cout << $$ << " -> " << $1 << endl;
  printf("label_%ld [label = \"%s\"]\n", label_counter, $2);
  printf("%s -> label_%ld\n", $$, label_counter);
  label_counter++;
}


